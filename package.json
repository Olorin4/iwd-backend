{
    "name": "ironwing-backend",
    "version": "1.0.0",
    "description": "Server side of iron-wing-dispatching.com handling form submissions, leads database and integration with CMR..",
    "type": "module",
    "main": "app.js",
    "scripts": {
        "start": "NODE_ENV=production node -r dotenv/config src/app.js",
        "start:dev": "dotenv -e .env.development -- node src/app.js",
        "test": "NODE_ENV=test jest --passWithNoTests",
        "test:integration": "NODE_ENV=test jest --testMatch '**/__tests__/**/*.integration.test.js'",
        "db:test:create": "psql -d postgres -c 'DROP DATABASE IF EXISTS iwd_test;' -U olorin4 -h localhost && psql -d postgres -c 'CREATE DATABASE iwd_test;' -U olorin4 -h localhost",
        "db:test:setup": "npx prisma migrate deploy",
        "lint": "eslint src --ext .js",
        "lint:fix": "eslint src --ext .js --fix",
        "clean": "rm -rf dist",
        "db:push:dev": "dotenv -e .env.development -- npx prisma db push",
        "db:migrate": "npx prisma migrate deploy",
        "prisma:generate": "node -r dotenv/config ./node_modules/.bin/prisma generate",
        "build": "babel src -d dist"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^6.4.1",
        "cors": "^2.8.5",
        "date-fns": "^3.6.0",
        "dotenv": "^16.4.7",
        "dotenv-flow": "^4.1.0",
        "express": "^4.21.2",
        "express-rate-limit": "^8.1.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "nodemailer": "^7.0.7",
        "morgan": "^1.10.0",
        "winston": "^3.13.0",
        "express-validator": "^7.2.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.25.9",
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@eslint/js": "^9.9.1",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.2.1",
        "concurrently": "^7.6.0",
        "dotenv-cli": "^8.0.0",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jest": "^28.9.0",
        "eslint-plugin-prettier": "^5.2.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.3.3",
        "prisma": "^6.17.1",
        "webpack": "^5.97.1"
    }
}
